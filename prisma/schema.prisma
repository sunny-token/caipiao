generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SSQResult {
  id          Int      @id @default(autoincrement())
  issueNumber String
  openDate    DateTime
  openNumbers Json
  ballOrder   String
  totalBet    String
  jackpot     String
  detail      String
  createdAt   DateTime @default(now())

  @@unique([issueNumber])
  @@index([openDate])
}

model DLTResult {
  id          Int      @id @default(autoincrement())
  issueNumber String
  openDate    DateTime
  openNumbers Json
  ballOrder   String
  totalBet    String
  jackpot     String
  detail      String
  createdAt   DateTime @default(now())

  @@unique([issueNumber])
  @@index([openDate])
}
