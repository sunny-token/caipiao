generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LotteryResult {
  id          Int      @id @default(autoincrement())
  issueNumber String   @unique // 期号唯一
  openDate    String // 开奖日期
  openNumbers Json // 开奖号码，已改为 Json 类型
  ballOrder   String // 出球顺序
  totalBet    String // 投注总额(元)
  jackpot     String // 奖池滚存(元)
  detail      String // 详情
  createdAt   DateTime @default(now())
}
